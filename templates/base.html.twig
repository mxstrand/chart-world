<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Welcome!{% endblock %}</title>
        <link rel="icon" href="data:image/svg+xml,<svg xmlns=%22http://www.w3.org/2000/svg%22 viewBox=%220 0 128 128%22><text y=%221.2em%22 font-size=%2296%22>⚫️</text></svg>">
        
        {# LUX #}
        <link rel="stylesheet" href="https://linkorb.github.io/lux/lux-latest.min.css">
        <script src="https://linkorb.github.io/lux/lux.bundle.js" defer></script>

        {% block stylesheets %}
            {{ encore_entry_link_tags('app') }}
        {% endblock %}

        {% block javascripts %}
            {{ encore_entry_script_tags('app') }}
        {% endblock %}
    </head>
    {% block menu %}
        {% include 'shared/lux-navbar.html.twig' with {'navItems': navItems} %}
    {% endblock menu%}
    <body>
        <div class="container-fluid">
            <div class="o-container u-pt-20">
                {% block body %}{% endblock %}
            </div>
        </div>
    </body>
</html>
